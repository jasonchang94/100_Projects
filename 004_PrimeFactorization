# Prime Factorization - Have the user enter a number and find all Prime Factors (if there are any) and display them.
def factors(number):
    fact = []
    for i in range(number):
        if number%(i+1) == 0:
            fact.append(int(i+1))

    return fact


def prime(num):
    for i in range(2,int(num)):
        if num % i == 0:
            return False
    else:
        return True


def PrimeFact():
    Number = int(input("Input a number for prime factorization"))
    Factors = factors(Number)
    Primefact = []
    for i in Factors:
        if prime(int(i)) == True:
            Primefact.append(i)
    if not Primefact:
        print("No primefactors")
    else:
        print("Factors are: " + str(Factors))
        print("Prime factors are: " + str(Primefact))

PrimeFact()
